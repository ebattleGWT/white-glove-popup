<script>
 function whiteGlovePop() {
  var popup = document.getElementById("wgPopup");
   popup.classList.remove("hide");
  popup.classList.toggle("show");
}
  function closeWG() {
  var popup = document.getElementById("wgPopup");
   popup.classList.remove("show");
  popup.classList.toggle("hide");
}
